#include <stdio.h>

int main() {
    int num, digit;

    // Input an integer
    printf("Enter an integer: ");
    scanf("%d", &num);

    printf("Digits of the integer %d are: ", num);

    // Extract and print each digit
    while (num != 0) {
        digit = num % 10; // Get the last digit
        printf("%d ", digit); // Print the digit
        num /= 10; // Remove the last digit
    }

    printf("\n");

    return 0;
}
